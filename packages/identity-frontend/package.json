{
  "name": "@libresat/identity-frontend",
  "version": "0.0.1-25",
  "description": "Frontend for @libresat/identity-backend, a simple authorization microservice",
  "author": "Felicitas Pojtinger <felicitas@pojtinger.com>",
  "license": "AGPL-3.0",
  "homepage": "https://gitlab.com/libresat/libresat/tree/master/packages/identity-frontend#README",
  "scripts": {
    "dev": "run-p dev:watch",
    "build": "run-p prod:build static:build",
    "start": "node dist/start.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:watch": "run-p static:watch",
    "dev:build": "run-p static:build",
    "dev:build:watch": "run-p static:build:watch",
    "dev:start": "ts-node -P tsconfig.json src/start.ts",
    "dev:start:watch": "nodemon -x 'run-p dev:start",
    "dev:start:debug": "node --inspect-brk=5858 ../../node_modules/.bin/ts-node --ignore false -P tsconfig.json src/start.ts",
    "dev:debug": "run-p dev:debug:watch",
    "dev:debug:watch": "nodemon -x 'run-p dev:build test dev:start:debug'",
    "prod:watch": "nodemon -x 'run-s prod:build prod:start'",
    "prod:build": "tsc --outDir dist/ src/start.ts",
    "prod:build:watch": "nodemon -x 'run-p prod:build'",
    "prod:start:watch": "nodemon -x 'run-p start'",
    "static:watch": "parcel src/index.html",
    "static:build:watch": "parcel watch src/index.html",
    "static:build": "parcel build src/index.html",
    "static:start": "ts-node -P tsconfig.json src/start.ts",
    "binary:build": "pkg package.json -t node10-linux-x64 -o bin/libresat-identity-frontend",
    "binary:start": "run-s binary:prepare binary:execute",
    "binary:install": "sudo install bin/libresat-identity-frontend /usr/bin",
    "binary:prepare": "chmod +x bin/libresat-identity-frontend",
    "binary:execute": "bin/libresat-identity-frontend",
    "prepare": "run-p build"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^3.0.2",
    "express": "^4.16.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.82.5",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "parcel-bundler": "^1.10.3",
    "typescript": "^3.1.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "pkg": "^4.3.4",
    "ts-node": "^7.0.1"
  },
  "main": "src/index.tsx",
  "source": "src/index.tsx",
  "bin": "dist/start.js",
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "scripts": "dist/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/libresat/libresat.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
