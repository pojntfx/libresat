{
  "name": "libresat",
  "description": "The Free/Libre and Open Source Satellite Development and Control Platform, since 2014",
  "version": "0.0.1-23",
  "main": "lerna.json",
  "scripts": {
    "dev": "lerna run dev --parallel",
    "dev:debug": "lerna run dev:debug --parallel",
    "build": "lerna run build --parallel",
    "build:dev": "lerna run build:dev --parallel",
    "build:binary": "lerna run build:binary --parallel",
    "start": "lerna run start --parallel",
    "start:dev": "lerna run start:dev --parallel",
    "start:binary": "lerna run start:binary --parallel",
    "install:binary": "lerna run install:binary --parallel",
    "watch": "lerna run watch --parallel",
    "watch:dev": "lerna run watch:dev --parallel",
    "watch:build": "lerna run watch:build --parallel",
    "watch:build:dev": "lerna run watch:build:dev --parallel",
    "watch:start": "lerna run watch:start --parallel",
    "watch:start:dev": "lerna run watch:start:dev --parallel",
    "prepare": "lerna bootstrap",
    "test": "lerna run test",
    "link": "lerna link",
    "clean": "lerna clean",
    "commit": "git add . && git-cz",
    "commit:staged": "git-cz",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "publish": "lerna publish --skip-git && npm run version && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/libresat/libresat.git"
  },
  "keywords": [
    "satellites",
    "space",
    "kubernetes",
    "cloud",
    "docker",
    "containers",
    "linux"
  ],
  "author": "Felicitas Pojtinger <felicitas@pojtinger.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^3.0.2",
    "conventional-changelog-cli": "^2.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "lerna": "^2.11.0",
    "nodemon": "^1.18.4",
    "pkg": "^4.3.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "bugs": {
    "url": "https://gitlab.com/libresat/libresat/issues"
  },
  "homepage": "https://gitlab.com/libresat/libresat#README",
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
