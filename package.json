{
  "name": "libresat",
  "description": "The Free/Libre and Open Source Satellite Development and Control Platform, since 2014",
  "version": "0.0.1-0",
  "main": "lerna.json",
  "scripts": {
    "dev": "lerna run dev --parallel",
    "dev:debug": "lerna run dev:debug --parallel",
    "build": "lerna run build --parallel",
    "build:dev": "lerna run build:dev --parallel",
    "build:binary": "lerna run build:binary --parallel",
    "start": "lerna run start --parallel",
    "start:dev": "lerna run start:dev --parallel",
    "start:binary": "lerna run start:binary --parallel",
    "install:binary": "lerna run install:binary --parallel",
    "watch": "lerna run watch --parallel",
    "watch:dev": "lerna run watch:dev --parallel",
    "watch:build": "lerna run watch:build --parallel",
    "watch:build:dev": "lerna run watch:build:dev --parallel",
    "watch:start": "lerna run watch:start --parallel",
    "watch:start:dev": "lerna run watch:start:dev --parallel",
    "prepare": "lerna bootstrap",
    "test": "lerna run test",
    "link": "lerna link",
    "clean": "lerna clean",
    "publish": "lerna publish"
