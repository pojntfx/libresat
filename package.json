{
  "name": "opensdcp-website",
  "version": "0.0.1-alpha",
  "description": "Main website for all the OpenSDCP projects.",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build":
      "next build && next export -o build/ && ncp .next/static build/_next/static",
    "start": "serve build/",
    "deploy": "npm run build && now --public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensdcp/opensdcp-website.git"
  },
  "keywords": [
    "opensdcp",
