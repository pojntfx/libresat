  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "lerna": "^2.11.0",
    "typescript": "^3.0.3",
    "nodemon": "^1.18.4",
    "pkg": "^4.3.4",
    "ts-node": "^7.0.1"
  },
  "bugs": {
    "url": "https://gitlab.com/libresat/libresat/issues"
  },
  "homepage": "https://gitlab.com/libresat/libresat#README",
  "publishConfig": {
    "access": "public"
  }
