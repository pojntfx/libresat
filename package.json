    "satellite",
    "space",
    "nextjs",
    "react",
    "reactjs",
    "frontend"
  ],
  "author": "Felix Pojtinger",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/opensdcp/opensdcp-website/issues"
  },
  "homepage": "https://github.com/opensdcp/opensdcp-website#readme",
  "dependencies": {
    "@zeit/next-sass": "^0.1.2",
    "bootstrap": "^4.0.0",
    "ncp": "^2.0.0",
    "next": "^5.0.0",
    "node-sass": "^4.8.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reactstrap": "^5.0.0-beta.3",
    "serve": "^6.5.3",
    "styled-components": "^3.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
